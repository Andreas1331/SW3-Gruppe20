<Window x:Class="SW3Projekt.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:models="clr-namespace:SW3Projekt.Models"
        Title="MainWindow" Height="450" Width="800">

    <Grid>
        <StackPanel Orientation="Horizontal" Height="35" HorizontalAlignment="Stretch" VerticalAlignment="Top">
            <Button Content="Create Timesheet" MinWidth="140" Click="BtnClickCreateTimesheet"/>
            <Button Content="View hired employees" MinWidth="140" Margin="10,0,0,0" Click="BtnClickViewHiredEmployees"/>
        </StackPanel>
        <Frame x:Name="Main" Margin="0, 35, 0, 0" NavigationUIVisibility="Visible" />
    </Grid>
</Window>
