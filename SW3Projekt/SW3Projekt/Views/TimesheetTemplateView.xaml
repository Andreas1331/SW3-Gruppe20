            <RowDefinition Height="110"></RowDefinition>
            <RowDefinition Height="*"></RowDefinition>
            <RowDefinition Height="42"></RowDefinition>
        </Grid.RowDefinitions>


            <StackPanel Grid.ColumnSpan="2" x:Name="TimesheetEntryTemplate" CanVerticallyScroll="True">
